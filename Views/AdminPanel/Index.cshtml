@model IEnumerable<Post>

@{ 

}

<h2>Admin Panel</h2>

<div>
    <ul>
        @foreach (var post in Model)
            {
        <li>
                @post.Title
            -
            <a asp-controller="AdminPanel" asp-action="Edit" asp-route-id="@post.Id">
                Edit
            </a>
            -
            <a asp-controller="AdminPanel" asp-action="RemovePost" asp-route-id="@post.Id">
                Remove
            </a>
        </li>
            }
    </ul>
</div>